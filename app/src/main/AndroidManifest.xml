<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.demoset">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TabLayoutActivity" />
        <activity android:name=".FloatingActionButtonActivity" />
        <activity android:name=".TextInputLayoutActivity" />
        <activity android:name=".SampleListPopupWindowActivity" />
        <activity
            android:name=".NavigationDrawerActivity"
            android:label="@string/title_activity_navigation_drawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AppBarLayoutActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ListPopupWindowActivity" />
        <activity android:name=".RecyclerViewLinearActivity" />
        <activity
            android:name=".ToolBarActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ToolbarSearchActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CollapsingToolbarLayoutActivity"></activity>
    </application>

</manifest>